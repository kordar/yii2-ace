{
    "name": "ace/kordar-ace",
    "description": "ace admin",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "perpel",
            "email": "572821520@qq.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-redis": "*",

        "kordar/upload": "dev-ace",
        "kordar/editormd": "1.0.0",

        "bower-asset/aceadmin": "1.1.1",
        "bower-asset/jquery-zclip": "1.1.*"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset":{
            "installer-paths": {
                "npm-asset-library": "vendor/npm",
                "bower-asset-library": "vendor/bower"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "kordar\\ace\\": ""
        }
    }
}
